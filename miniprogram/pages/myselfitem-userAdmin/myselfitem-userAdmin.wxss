/* pages/myselfitem-userAdmin/userAdmin.wxss */
Page{
  margin:0;
  padding:0;
}
.container{
  width:100%;
  height:100%;
  margin:0;
  padding:0 20rpx;
  font-size:12px;
}
.nav{
  width:100%;
  height:80rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-bottom:1px solid #ddd;
  margin-bottom:10rpx;
}
.nav view{
  text-align: center;
  height:80rpx;
  line-height:90rpx;
  margin:20rpx;
  font-size:13px;
}
.user{
  width:100%;
  padding:20rpx;
  border-bottom:1px solid #ddd;
  margin:5px;
  height:100rpx;
}
.avatar_url{
  width:100rpx;
  height:100rpx;
  float:left;
}
.avatar_url image{
  width:100rpx;
  height:100rpx;
}
.nick_name{
  float:left;
  margin-left:10rpx; 
}
.telphone{
  float:left;
  margin-left:20rpx;
}
.switch{
  float:right;
  line-height:100rpx;
  
}
.switch text{
  margin-right:20px;
}
.tip{
  height:30px;
  line-height:30px;
  text-align:center;
}